<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Company>Internet Sukces Piotr Stęclik</Company>
    <Copyright>Copyright © Internet Sukces Piotr Stęclik 2017-2023</Copyright>
    <Product>iSukces.UnitedValues</Product>
    <TargetFrameworks>net48;net6.0;netcoreapp3.1;netstandard2.0;net7.0</TargetFrameworks>
    <AssemblyVersion>1.23.1125.32</AssemblyVersion>
    <FileVersion>1.23.1125.32</FileVersion>
    <Version>1.23.1125.32</Version>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>key.snk</AssemblyOriginatorKeyFile>
    <NoWarn>1701;1702;1591</NoWarn>
    <Authors>Internet Sukces Piotr Stęclik</Authors>
    <Description>Values with units serialization and simple algebra</Description>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSource>false</IncludeSource>
    <IncludeSymbols>true</IncludeSymbols>
    <PackageId>iSukces.UnitedValues</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/isukces/isukces.UnitedValues</RepositoryUrl>
    <PackageProjectUrl>https://github.com/isukces/isukces.UnitedValues</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2023.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="+IUnitedValue\+inversed\InversedDeltaKelvinTemperature.auto.cs">
      <DependentUpon>InversedDeltaKelvinTemperature.cs</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>
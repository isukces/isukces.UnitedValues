{
    public class AlgebraTests
    {
<#

foreach(var i in tests2) {
#>
        [Fact]
        public void ShoulDivide<#= i[0] #>()
        {
			var a = new <#= i[0] #>(10m, <#= i[1] #>);
			var b = new <#= i[0] #>(5m, <#= i[1] #>);
			var c = a / b;
			Assert.Equal(2, c);
        }
<# } #>
	}
